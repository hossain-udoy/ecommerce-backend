// .eslintrc
{
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
	"rules": {
		"no-unused-vars": "error",
        "prefer-const":"error",
        "no-unused-expressions":"error",
        "no-undef":"error",
        "no-console":"warn"
    
	},
	"globals": {
		"process": "readonly"
	}
}
